{
  "name": "hekla",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:env": "sh ./scripts/build-env.sh",
    "build:e2e": "detox build --configuration $E2E_CONFIG",
    "build:e2e:ios": "E2E_CONFIG=ios.sim.release yarn build:e2e",
    "build:e2e:android": "E2E_CONFIG=android.emu.release yarn build:e2e",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "xcode": "open ./ios/Hekla.xcworkspace",
    "test": "jest",
    "test:e2e": "detox test --configuration $E2E_CONFIG",
    "test:e2e:ios": "E2E_CONFIG=ios.sim.release yarn test:e2e",
    "test:e2e:android": "E2E_CONFIG=android.emu.release yarn test:e2e",
    "tslint": "tslint src/**/*.ts src/**/*.tsx",
    "eslint": "eslint src __tests__",
    "lint": "yarn tslint && yarn eslint",
    "precommit": "lint-staged",
    "prepush": "npm run test",
    "prepare": "patch-package"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ],
    "*.{ts,tsx}": [
      "tslint"
    ]
  },
  "eslintIgnore": [
    "config.env.js"
  ],
  "dependencies": {
    "core-decorators": "0.20.0",
    "date-fns": "1.29.0",
    "firebase": "5.1.0",
    "html-entities": "1.2.1",
    "jsc-android": "224109.0.0",
    "lodash": "4.17.10",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "mobx-state-tree": "2.2.0",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-native": "0.55.4",
    "react-native-blur": "3.2.2",
    "react-native-cheerio": "1.0.0-rc.4",
    "react-native-code-push": "5.3.5",
    "react-native-config": "0.11.5",
    "react-native-cookies": "^3.3.0",
    "react-native-fast-image": "4.0.14",
    "react-native-firebase": "4.2.0",
    "react-native-haptic-feedback": "1.1.7",
    "react-native-interactable": "0.1.10",
    "react-native-keychain": "3.0.0-rc.3",
    "react-native-navigation": "2.0.2366",
    "react-native-progress": "3.5.0",
    "react-native-safari-view": "2.1.0",
    "react-native-search-bar": "3.4.2",
    "react-native-sentry": "0.38.1",
    "react-native-vector-icons": "4.6.0",
    "recyclerlistview": "1.3.4"
  },
  "devDependencies": {
    "@firebase/app-types": "0.3.2",
    "@types/core-decorators": "^0.20.0",
    "@types/date-fns": "^2.6.0",
    "@types/detox": "^7.3.0",
    "@types/enzyme": "3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/html-entities": "1.2.16",
    "@types/jest": "23.1.3",
    "@types/lodash": "4.14.110",
    "@types/mocha": "5.2.3",
    "@types/prop-types": "^15.5.3",
    "@types/react": "16.4.4",
    "@types/react-native": "0.55.26",
    "@types/react-native-navigation": "1.1.12",
    "@types/react-test-renderer": "^16.0.1",
    "@types/webpack-env": "1.13.6",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-jest": "23.2.0",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "css-to-react-native-transform": "1.6.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.0.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-detox": "1.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.17.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-react-native": "3.2.1",
    "husky": "0.14.3",
    "jest": "23.2.0",
    "lint-staged": "7.2.0",
    "mocha": "5.2.0",
    "patch-package": "5.1.1",
    "postinstall-prepare": "1.0.1",
    "react-dom": "16.4.1",
    "react-native-css-transformer": "1.2.1",
    "react-native-rename": "2.2.2",
    "react-native-stylus-transformer": "1.1.1",
    "react-native-typescript-transformer": "1.2.10",
    "react-test-renderer": "16.4.1",
    "stylus": "0.54.5",
    "ts-jest": "22.4.6",
    "tslib": "1.9.3",
    "tslint": "5.10.0",
    "tslint-config-airbnb": "5.9.2",
    "tslint-react": "3.6.0",
    "typescript": "2.9.2"
  },
  "resolutions": {
    "@types/react": "16.3.16"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.styl$": "<rootDir>/scripts/jest-css-modules.js"
    },
    "preset": "react-native",
    "modulePaths": [
      "<rootDir>"
    ],
    "setupFiles": [
      "./scripts/setup-jest.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "css"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json",
        "useBabelrc": true
      }
    }
  },
  "detox": {
    "test-runner": "mocha",
    "specs": "__tests__",
    "runner-config": "__tests__/mocha.opts",
    "configurations": {
      "ios.sim.release": {
        "binaryPath": "ios/build/Build/Products/Release-iphonesimulator/Hekla.app",
        "build": "export RCT_NO_LAUNCH_PACKAGER=true && xcodebuild -workspace ios/Hekla.xcworkspace -scheme Hekla -configuration Release -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      },
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/Hekla.app",
        "build": "xcodebuild -workspace ios/Hekla.xcworkspace -scheme Hekla -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build -quiet",
        "type": "ios.simulator",
        "name": "iPhone 7 Plus"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "pushd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && popd",
        "type": "android.emulator",
        "name": "Nexus_5_API_27"
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "pushd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && popd",
        "type": "android.emulator",
        "name": "Nexus_5_API_27"
      }
    }
  }
}
